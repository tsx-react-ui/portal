{
    "name": "@tsx-react-ui/portal",
    "version": "1.0.0",
    "author": {
        "name": "maqing01",
        "email": "475986855@qq.com"
    },
    "repository": {
        "type": "git",
        "url": "git+http://gitub.com/tsx-react-ui/portal.git"
    },
    "group": "tsx-react-ui",
    "type": "react",
    "keywords": [
        "earth",
        "react",
        "reactcomponent",
        "portal"
    ],
    "main": "dist/index.js",
    "peerDependencies": {
        "react": ">=16.4.0",
        "react-dom": ">=16.4.0",
        "core-js": ">=2.5.7",
        "raf": ">=3.4.0"
    },
    "dependencies": {
        "classnames": "^2.2.5",
        "pinkie-promise": "^2.0.1",
        "promise": "8.0.1",
        "prop-types": "^15.5.10",
        "whatwg-fetch": "2.0.3"
    },
    "scripts": {
        "start": "ts-component-script start",
        "build": "ts-component-script build",
        "test": "ts-component-script test --env=jsdom"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx}"
        ],
        "setupFiles": [
            "<rootDir>/config/polyfills.js"
        ],
        "testMatch": [
            "<rootDir>/**/__tests__/**/*.js?(x)",
            "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
        ],
        "testEnvironment": "node",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/node_modules/ts-component-script/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/ts-component-script/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
        ],
        "moduleNameMapper": {
            "^react-native$": "react-native-web"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "devDependencies": {
        "@types/classnames": "^2.2.6",
        "@types/react": "^16.4.14",
        "@types/react-dom": "^16.0.7",
        "core-js": "^2.5.7",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "lm-flexible": "^0.1.1",
        "object-assign": "4.1.1",
        "raf": "^3.4.0",
        "react": "^16.4.0",
        "react-dom": "^16.4.0",
        "sinon": "^3.2.1",
        "ts-component-script": "^1.0.0",
        "tslib": "^1.9.3",
        "typescript": "^3.0.3"
    },
    "proxy": "http://localhost:3001/"
}
